# UCM AI Backend Environment Variables
# Copy this file to .env and update with your values

# Database Configuration
# For PostgreSQL (Docker):
DATABASE_URL=postgresql+psycopg2://ucm:ucm@db:5432/ucmdb
# For SQLite (Local):
# DATABASE_URL=sqlite:///./ucm.db

# Model Service URL
MODEL_SVC_URL=http://model_service:9000

# Admin API Key (Change in production!)
ADMIN_API_KEY=dev-admin-key

# Privacy Header Logging
# Set to true to log privacy headers (GPC, DNT, etc.)
PRIVACY_LOGGING=false

# Server Configuration
HOST=0.0.0.0
PORT=8000
